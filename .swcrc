{
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "decorators": true,
      "dynamicImport": true
    },
    "transform": {
      "legacyDecorator": true,
      "decoratorMetadata": true
    }
  },
  "env": {
    "targets": {
      "node": "19"
    }
  },
  "sourceMaps": "inline",
  "module": {
    "type": "commonjs"
  }
}
